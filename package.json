{
  "name": "review-app",
  "version": "1.0.0",
  "description": "Shows order information to review",
  "main": "index.js",
  "scripts": {
    "start": "node server-dist/server-bundle.js",
    "dev": "NODE_ENV=development node shim.js && npm-run-all -p dev:build:server dev:run:server",
    "dev:run:server": "nodemon --watch dist/server --exec \"node dist/server/server-bundle.js\"",
    "dev:build:server": "NODE_ENV=development webpack --mode development --watch --config webpack.server.js",
    "dev:build:client": "NODE_ENV=development webpack --mode development --config webpack.client.js",
    "prod": "node shim.js && npm-run-all -s prod:build:server prod:build:client prod:server",
    "prod:server": "node server-dist/server-bundle.js",
    "prod:build:server": "NODE_ENV=production webpack --mode production --config webpack.server.js",
    "prod:build:client": "NODE_ENV=production webpack --mode production --config webpack.client.js",
    "test": "jest",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "babel": "^6.23.0",
    "babel-jest": "^25.1.0",
    "core-js": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-http-proxy": "^1.6.0",
    "foundation-sites": "^6.5.3",
    "html-react-parser": "^0.10.0",
    "is-object": "^1.0.1",
    "jest": "^25.1.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.19.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-awesome-slider": "^3.1.2",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-native-image-slider-show": "^1.0.3",
    "react-redux": "^7.1.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.12.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "regenerator": "^0.14.2",
    "regenerator-runtime": "^0.13.3",
    "serialize-javascript": "^2.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.js",
    "moduleNameMapper": {
      "\\.(css|scss|png|jpg|jpeg)$": "<rootDir>/__test__/setup/styleMock.js"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "motion-ui": "^2.0.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "ts-loader": "^6.2.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
